    "collectionSchemeListTopic": "$aws/iotfleetwise/vehicles/$VEHICLE_ID/collection_schemes",
    "decoderManifestTopic": "$aws/iotfleetwise/vehicles/$VEHICLE_ID/decoder_manifests",
    "canDataTopic": "$aws/iotfleetwise/vehicles/$VEHICLE_ID/signals",
    "checkinTopic": "$aws/iotfleetwise/vehicles/$VEHICLE_ID/checkins",
    "certificateFilename": "/etc/aws-iot-fleetwise-sim/$VEHICLE_ID/cert.crt",
    "privateKeyFilename": "/etc/aws-iot-fleetwise-sim/$VEHICLE_ID/pri.key"
  }
}
